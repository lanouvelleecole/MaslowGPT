import { TestJobSingletonInstance } from "./TestJobSingleton";

/**
 * Ceci permet de cancel ce Work (nommé TestJob)
 */
export const CancelTestJob = () => {
  const controller = TestJobSingletonInstance.GetCanceller("TestJob");

  controller?.abort();

  //console.log(`Annulation de TestJob effectuée avec succès !`);
};
